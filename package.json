  },
  "files": [
    "build/",
    "lib/config.js"
  ],
  "dependencies": {
    "@rcsb/rcsb-molstar": "^2.5.3",
    "@rcsb/rcsb-saguaro": "^2.0.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "classnames": "^2.3.1",
    "lodash.clonedeep": "^4.5.0",
    "pako": "^2.0.3",
    "rc-collapse": "^2.1.0",
    "rc-dropdown": "^3.2.0",
    "rc-menu": "^8.10.2",
    "rc-select": "^11.5.0",
    "rc-tabs": "^11.11.0",
    "rc-upload": "^3.3.1",
    "react": "17.0.2",
    "react-autosuggest": "^10.1.0",
    "react-dom": "17.0.2",
    "rxjs": "^7.4.0"
  },
  "overrides": {
    "@rcsb/rcsb-molstar": {
      "react": "17.0.2",
      "react-dom": "17.0.2"
    }
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.0",
    "@graphql-codegen/cli": "^2.9.1",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@graphql-tools/apollo-engine-loader": "^7.3.4",
    "@graphql-tools/github-loader": "^7.3.4",
    "@graphql-tools/prisma-loader": "^7.2.4",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/pako": "^2.0.0",
    "@types/react-autosuggest": "^10.1.5",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "argparse": "^2.0.1",
    "concurrently": "^6.4.0",
    "cpx2": "^4.0.0",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "extra-watch-webpack-plugin": "^1.0.3",
    "file-loader": "^6.2.0",
    "graphql": "^15.8.0",
    "http-server": "^14.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "mini-css-extract-plugin": "^2.4.5",
    "path-browserify": "^1.0.1",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "codegen-gql": "graphql-codegen --config src/auto/data/codegen.yml",
    "clean": "node ./scripts/clean.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build-tsc": "tsc",
    "build-extra": "cpx \"src/**/*.{css,html,ico}\" lib/",
    "build-webpack": "webpack --mode production",
    "build": "npm run build-tsc && npm run build-extra && npm run build-webpack",
    "watch-tsc": "tsc --watch --incremental",
    "watch-extra": "cpx \"src/**/*.{css,html,ico}\" lib/ --watch",
    "watch-webpack": "webpack -w --mode development --stats minimal",
    "serve": "http-server -p 3000 -g",
    "watch-app": "concurrently -c \"green,gray,gray\" --names \"tsc,ext,wpc\" --kill-others \"npm:watch-tsc\" \"npm:watch-extra\" \"npm:watch-webpack\"",
    "version": "npm run clean && npm run build",
    "postversion": "git push && git push --tags"
  }
}
